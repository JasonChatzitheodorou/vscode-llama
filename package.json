{
    "name": "llama-language-support",
    "displayName": "Llama Language Support",
    "publisher": "JasonChatzitheodorou",
    "icon": "icons/icons8-llama-64.png",
    "description": "",
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "llama",
                "aliases": [
                    "Llama",
                    "llama"
                ],
                "extensions": [
                    ".lla"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "llama",
                "scopeName": "source.lla",
                "path": "./syntaxes/llama.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JasonChatzitheodorou/vscode-llama.git"
    }
}
